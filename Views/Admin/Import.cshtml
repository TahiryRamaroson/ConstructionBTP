@{
    ViewData["Title"] = "Import";
    Layout = "_Layout_Admin";
}


<div class="container">

    <div class="row mt-10">
		<div class="col-md-5 offset-md-1">
			<div class="text-center mt-10">
				<h1>Importation données</h1>
			</div>
			<form asp-controller="Admin" asp-action="ImportDonnee" method="post" class="form w-100" enctype="multipart/form-data">
				

				<div class="fv-row mb-10">
					<label class="form-label fs-6 fw-bold text-dark">Maison et travaux</label>
					<input class="form-control form-control-lg form-control-solid" type="file" name="maisonTravaux" autocomplete="off" />
				</div>

				<div class="fv-row mb-10">
					<div class="d-flex flex-stack mb-2">
						<label class="form-label fw-bold text-dark fs-6 mb-0">Devis</label>
					</div>
					<input class="form-control form-control-lg form-control-solid" type="file" name="devis" autocomplete="off" />
				</div>

				<div class="text-center">
					<button type="submit" class="btn btn-lg btn-primary w-100 mb-5">
						<span class="indicator-label">Importer</span>
					</button>
				</div>
			</form>
        </div>

		<div class="col-md-5 offset-md-1">
			<div class="text-center mt-10">
				<h1>Importation Paiement</h1>
			</div>
			<form asp-controller="Admin" asp-action="ImportPaiement" method="post" class="form w-100" enctype="multipart/form-data">


				<div class="fv-row mb-10">
					<label class="form-label fs-6 fw-bold text-dark">Paiement</label>
					<input class="form-control form-control-lg form-control-solid" type="file" name="paiement" autocomplete="off" />
				</div>

				<div class="text-center">
					<button type="submit" class="btn btn-lg btn-primary w-100 mb-5">
						<span class="indicator-label">Importer</span>
					</button>
				</div>
			</form>
        </div>
    </div>

    